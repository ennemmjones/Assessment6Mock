@model List<Employee>

@{
    ViewData["Title"] = "Home Page";


}

<div class="text-center">
    <h1 class="display-4">Employee</h1>
   
</div>

<table>
    <tr>
        <th>First Name</th>
        <th>Age</th>
        <th>Salary</th>
    </tr>
    @foreach (var m in Model)
    {
        <tr>
            <td>@m.Id</td>
            <td>@m.FirstName</td>
            <td>@m.Age</td>
            <td>@m.Salary</td>
        </tr>

    }

</table>


<form action="/Home/RetirementInfo" method="post">
    <input type="number" name="id" id="id" />
    <button class="btn btn-primary">Submit</button>

</form>
